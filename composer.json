{
  "name": "bytic/codeception",
  "description": "Php Framework",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "bytic",
    "codeception"
  ],
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "homepage": "https://github.com/ByTIC/Codeception",
  "require": {
    "php": "^7.4 || ^8.0",
    "codeception/codeception": "^5.0",
    "codeception/mockery-module": "^0.5",
    "codeception/module-asserts": "^2.0 || ^3.0",
    "codeception/module-db": "^2.0 || ^3.0",
    "codeception/module-phpbrowser": "^2.0 || ^3.0",
    "codeception/module-rest": "^3.0",
    "codeception/verify": "^2.0||^3.0"
  },
  "require-dev": {
    "ext-pdo": "*",
    "codeception/specify": "^2.0",
    "bytic/phpqatools": "^1.0",
    "symfony/filesystem": "^5.0 || ^6.0 || ^7.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "ByTIC\\Codeception\\Support\\": "src/Support",
      "ByTIC\\Codeception\\Extensions\\": "src/Extensions",
      "ByTIC\\Codeception\\Helper\\": "src/tests/_support/Helper",
      "ByTIC\\Codeception\\Page\\": "src/tests/_support/Page",
      "ByTIC\\Codeception\\Acceptance\\": "src/tests/acceptance"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "phpstan/extension-installer": true,
      "bytic/phpqatools": true
    },
    "sort-packages": true
  },
  "scripts": {}
}
